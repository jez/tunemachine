{
  "name": "tunemachine",
  "version": "0.9.0",
  "description": "A time machine for your Spotify tunes.",
  "keywords": ["spotify", "time machine", "version control"],
  "license": "MIT",
  "main": "server.coffee",
  "scripts": {
    "watchify": "watchify -t coffee-reactify --debug ./public/src/app.cjsx -o public/js/bundle.js -v",
    "browserify": "browserify -t coffee-reactify ./public/src/app.cjsx | uglifyjs > public/js/bundle.js",
    "build": "npm run browserify ",
    "dev": "npm run watchify & nodemon -e coffee"
  },
  "author": "CMU A16Z 2015 Hackathon Team",
  "contributors": [
    "Oscar Bezi <oscar@bezi.io>",
    "Justin Gallagher <justingallag@gmail.com>",
    "Min Zhou <minzhou@andrew.cmu.edu>",
    "Jacob Zimmerman <jake@zimmerman.io>"
  ],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.1",
    "connect-mongo": "^0.8.1",
    "express": "^4.12.4",
    "express-session": "^1.11.3",
    "jade": "^1.11.0",
    "jquery": "^2.1.4",
    "moment": "^2.10.3",
    "mongoose": "^4.0.5",
    "morgan": "^1.6.0",
    "react": "^0.13.3",
    "request": "^2.58.0",
    "serve-favicon": "^2.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "coffee-react": "^3.2.0",
    "coffee-reactify": "^3.0.0",
    "uglify-js": "^2.4.23",
    "watchify": "^3.2.2"
  }
}
